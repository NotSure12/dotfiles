
shopt -s expand_aliases # needed for integrating inside of vim
alias be='bundle exec'
alias bi='bundle install'
alias bu='bundle update'
alias bem='be rake db:migrate'
alias bet='be rake db:test:prepare'
alias gs='git st'
alias gd='git di'
alias gb='git br'
alias gba='git br -a'
alias gcm='git ci -m'
alias gca='git ci -a'
alias gcam='git ci -am'
alias rc='rvm current'
alias lss='ls -alt *.sess'
alias flushdns='sudo killall -HUP mDNSResponder'
alias mem='/usr/local/opt/memcached/bin/memcached -vv'
alias redis='/usr/local/redis-2.6.16/src/redis-server'
alias rkiq='bundle exec sidekiq -q high,5 -q default, -c 1' # basic development sidekiq

alias soa='source ~/.bash/aliases'
alias voa='vim ~/.bash/aliases'
alias coa='cat ~/.bash/aliases'
