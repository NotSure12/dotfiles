
shopt -s expand_aliases # needed for integrating inside of vim
alias be='bundle exec'
alias bi='bundle install'
alias bu='bundle update'
alias flushdns='sudo killall -HUP mDNSResponder'
alias mem='/usr/local/opt/memcached/bin/memcached -vv'
alias redis='/usr/local/redis-2.6.16/src/redis-server'
alias rkiq='bundle exec sidekiq -q high,5 -q default, -c 1' # basic development sidekiq
